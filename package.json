{
  "name": "uni-boot-plus-app",
  "version": "0.0.1",
  "description": "uni-boot-plus 配套 Vue3 前端骨架",
  "author": {
    "name": "Cade",
    "email": "cadecode@foxmail.com",
    "url": "https://github.com/cadecode"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cadecode/uni-boot-plus-app.git"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:stage": "vue-tsc --noEmit && vite build --mode staging",
