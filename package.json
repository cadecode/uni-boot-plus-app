    "prettier": "3.3.3",
    "sass": "1.77.8",
    "typescript": "5.5.4",
    "unocss": "0.62.2",
    "vite": "5.4.2",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-svg-loader": "5.1.0",
    "vitest": "2.0.5",
    "vue-eslint-parser": "9.4.3",
    "vue-tsc": "2.0.29"
  },
  "keywords": [
    "vue",
    "vue3",
    "admin",
    "vue-admin",
    "vue3-admin",
    "vite",
